<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>

	<settings>
		<setting name="cacheEnabled" value="true" />
		<setting name="lazyLoadingEnabled" value="false" />
		<setting name="multipleResultSetsEnabled" value="true" />
		<setting name="useColumnLabel" value="true" />
		<setting name="useGeneratedKeys" value="false" />
		<setting name="defaultExecutorType" value="SIMPLE" />
		<setting name="defaultStatementTimeout" value="25000" />
		<setting name="mapUnderscoreToCamelCase" value="true" />
		<setting name="jdbcTypeForNull" value="NULL" />
		<setting name="logImpl" value="LOG4J2" />
		<setting name="callSettersOnNulls" value="true" />
	</settings>

	<typeAliases>
		<typeAlias alias="hashMap" type="java.util.HashMap"/>
		<typeAlias alias="commonMap" type="egovframework.common.vo.CommonHashMap" />
    	<typeAlias alias="searchVO" type="egovframework.common.vo.SearchVO" />
		<typeAlias alias="sampleVO" type="egovframework.sample.vo.SampleVO" />
		<typeAlias alias="loginUserVO" type="egovframework.common.login.vo.LoginUserVO" />
		<typeAlias alias="memberVO" type="egovframework.member.vo.MemberVO" />
		<typeAlias alias="attachVO" type="egovframework.attach.vo.AttachVO" />
	</typeAliases>

</configuration>